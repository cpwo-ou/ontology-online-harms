###  http://cpwo.open.ac.uk/ontology/ohh#Act
:Act rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :hasApplicableLegistableClause ;
                       owl:allValuesFrom :LegislativeClause
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :isInfluencedBy ;
                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onClass :ContextualFactor
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :occursIn ;
                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onClass :Space
                     ] ;
     rdfs:comment "Primary class representing the legal concept of a criminal act. A crime requires both actus reus and mens rea" ;
     rdfs:label "Act"@en-gb .


