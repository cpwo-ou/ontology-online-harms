###  http://cpwo.open.ac.uk/ontology/ohh#ethnicity
:ethnicity rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range [ owl:intersectionOf ( skos:Concept
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty skos:inScheme ;
                                               owl:hasValue ooh-kos:Ethnicity
                                             ]
                                           ) ;
                        rdf:type owl:Class
                      ] .


###  http://cpwo.open.ac.uk/ontology/ohh#gender
:gender rdf:type owl:ObjectProperty ;
        rdfs:domain :Person ;
