###  http://cpwo.open.ac.uk/ontology/ohh#ethnicity
:ethnicity rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range [ owl:intersectionOf ( skos:Concept
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty skos:inScheme ;
                                               owl:hasValue ooh-kos:EthnicityGroup
                                             ]
                                           ) ;
                        rdf:type owl:Class
                      ] ;
           rdfs:comment "Ethnicity is wider than race and refers to shared cultural experiences, religious beliefs, traditions, ancestry, language dialect or national origins (e.g., African or Irish). Recommended concept by the Government Statistical Service (GSS)"@en-gb ;
           rdfs:label "ethnicity"@en-gb .


